{
  "name": "tastyigniter/ti-theme-orange",
  "type": "tastyigniter-theme",
  "description": "Free Modern, Responsive and Clean TastyIgniter Theme based on Bootstrap.",
  "license": "MIT",
  "authors": [
    {
      "name": "TastyIgniter Dev Team"
    }
  ],
  "keywords": [
    "tastyigniter",
    "theme",
    "orange"
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "pestphp/pest": "^1.20"
  },
  "extra": {
    "tastyigniter-theme": {
      "code": "tastyigniter-orange",
      "name": "Orange Theme",
      "locked": true,
      "require": {
        "igniter.cart": "*",
        "igniter.coupons": "*",
        "igniter.frontend": "*",
        "igniter.local": "*",
        "igniter.pages": "*",
        "igniter.payregister": "*",
        "igniter.reservation": "*",
        "igniter.user": "*"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
